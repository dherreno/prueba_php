pipeline {
  agent {
        docker { image 'hello-php:latest' }
    }

  
  stages {
    stage('install') {
      steps {
        echo install
        }
      }

    stage('test') {
      steps {
        echo 'test'
        }
      }

    stage('build') {
      steps {
        echo 'build'
        }
      }

    stage('deploy') {
      steps {
        sh 'docker run -p 5009:80 -d -v D:\\\\Usuarios\\\\analista.tic3\\\\Desktop\\\\docker-php/src:/var/www/html/  hello-php --privileged'
      }
    }
  }

}
